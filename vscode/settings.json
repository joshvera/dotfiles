{
    "editor.fontFamily": "Inconsolata, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 18,
    "editor.minimap.enabled": false,
    "editor.rulers": [
        120
    ],
    "workbench.colorTheme": "GitHub Light",
    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#009688",
        "syntax.type": "#d73a49",
        "syntax.scope": "#24292eff",
        "syntax.function": "#6182B8",
        "syntax.variable": "#607D8B",
        "syntax.number": "#005cc5",
        "syntax.string": "#032f62",
        "syntax.comment": "#6a737d",
        "syntax.constant": "#FF5370",
        "syntax.directive": "#24292eff",
        "syntax.control": "#7986CB",
        "syntax.operator": "#39ADB5",
        "syntax.modifier": "#d73a49",
        "syntax.punctuation": "#d73a49",
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "keyword.other",
                "settings": {
                    "foreground": "#7C4DFF"
                }
            },
            {
                "scope": "storage.type",
                "settings": {
                    "foreground": "#607D8B",
                }
            },
            {
                "scope": "constant.other",
                "settings": {
                    "foreground": "#FF5370",
                }
            },
            {
                "scope": "keyword.operator",
                "settings": {
                    "foreground": "#39ADB5",
                }
            },
            {
                "scope": "source",
                "settings": {
                    "foreground": "#607D8B",
                }
            },
            {
                "scope": "meta.type-declaration",
                "settings": {
                    "foreground": "#39ADB5"
                }
            },
            {
                "scope": "variable.other.generic-type",
                "settings": {
                    "foreground": "#80CBC4"
                }
            },
            {
                "scope": "entity.name.function",
                "settings": {
                    "foreground": "#6182B8"
                }
            }
        ]
    },
    "git.confirmSync": false,
    "git.autofetch": true,
    "files.autoSave": "onFocusChange",
    "editor.wordWrapColumn": 120,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "breadcrumbs.enabled": true,
    "search.smartCase": true,
    "search.quickOpen.includeSymbols": true,
    "vs-kubernetes": {
        "vs-kubernetes.kubectl-path": "/Users/vera/.vs-kubernetes/tools/kubectl/kubectl",
        "vs-kubernetes.helm-path": "/Users/vera/.vs-kubernetes/tools/helm/darwin-amd64/helm",
        "vs-kubernetes.draft-path": "/Users/vera/.vs-kubernetes/tools/draft/darwin-amd64/draft",
        "vs-kubernetes.minikube-path": "/Users/vera/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vs-kubernetes.minikube-path.mac": "/Users/vera/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vscode-kubernetes.kubectl-path.mac": "/Users/vera/.vs-kubernetes/tools/kubectl/kubectl",
        "vscode-kubernetes.helm-path.mac": "/Users/vera/.vs-kubernetes/tools/helm/darwin-arm64/helm",
        "vscode-kubernetes.minikube-path.mac": "/Users/vera/.vs-kubernetes/tools/minikube/darwin-arm64/minikube"
    },
    "[haskell]": {
        "editor.defaultFormatter": "sjurmillidahl.ormolu-vscode"
    },
    "[go]": {
        "editor.snippetSuggestions": "none",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "files.eol": "\n",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "yaml.format.enable": false,
    "C_Cpp.updateChannel": "Insiders",
    "atomKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": true,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust"
    },
    "terraform.languageServer": {
        "external": true,
        "args": [
            "serve"
        ]
    },
    "ghcSimple.replCommand": "cabal new-repl semantic",
    "ghcSimple.inlineRepl.loadType": "object-code",
    "go.formatTool": "goimports",
    "go.useLanguageServer": true,
    "python.languageServer": "Pylance",
    "[python]": {
        "editor.defaultFormatter": "mikoz.black-py",
        "editor.formatOnType": true
    },
    "workbench.settings.openDefaultKeybindings": true,
    "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "workbench.editor.untitled.hint": "hidden",
    "codeQL.telemetry.enableTelemetry": false,
    "ghcSimple.trustedReplCommandConfigs": {
        "/Users/vera/github/dotfiles": "cabal new-repl semantic"
    },
    "idris.executablePath": "idris2",
    "redhat.telemetry.enabled": false,
    "[css]": {
        "editor.suggest.insertMode": "replace",
        "gitlens.codeLens.scopes": [
            "document"
        ]
    },
    "css.format.enable": false,
    "python.venvFolders": [
        "/Users/vera/.cache/pypoetry/virtualenvs/",
        "/Users/vera/Library/Caches/pypoetry/virtualenvs/app-wS8lqT4F-py3.10"
    ],
    "emmet.showExpandedAbbreviation": "never",
    "emmet.preferences": {},
}
